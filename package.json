{
  "version": "1.0.0",
  "name": "strapi-plugin-webhook-button",
  "description": "Display a button in the content-type view that triggers a webhook.",
  "license": "MIT",
  "author": "douwepausma <mail@douwepausma.nl>",
  "keywords": [],
  "type": "commonjs",
  "strapi": {
    "kind": "plugin",
    "name": "webhook-button",
    "displayName": "Webhook Button",
    "description": "Display a button in the content-type view that triggers a webhook."
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react-intl": "^7"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.36.0",
    "@strapi/sdk-plugin": "^5.4.0",
    "prettier": "^3.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@strapi/typescript-utils": "^5.36.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.36.0",
    "@strapi/sdk-plugin": "^5.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.9"
  }
}